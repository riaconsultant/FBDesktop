<BaseShape xmlns="com.roguedevelopment.flowchart.shapes.*" 
		   xmlns:mx="http://www.adobe.com/2006/mxml"
		   xmlns:degrafa="com.degrafa.*"
		   xmlns:geometry="com.degrafa.geometry.*" 
		   width="5" height="50" xmlns:degrafa1="http://www.degrafa.com/2007">
		   
		   <degrafa:Surface width="100%" height="100%">
		   		<degrafa:GeometryGroup>
		   			<geometry:RegularRectangle 	   			
		   					 fill="{background}"
		   					 stroke="{stroke}"
		   					 width="{width}"
		   					 height="{height}"
		   					/>
		   		</degrafa:GeometryGroup>
		   </degrafa:Surface>
	
</BaseShape>
