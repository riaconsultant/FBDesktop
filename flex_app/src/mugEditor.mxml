				rect.graphics.lineStyle(3,color);
				if(mug == genericMug)
				{
					ui.x = _x;
					ui.y = _y-3;
					rect.graphics.moveTo(0,1);
					rect.graphics.lineTo(_w+2,1);
					rect.graphics.moveTo(2,_h+4);
					rect.graphics.lineTo(_w-3,_h+4);
				}
				if(mug == genericMugLeft)
