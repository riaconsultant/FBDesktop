							if(mug == genericMug){
								childObj.x = _x1 + x1;
							}if(mug == genericMugLeft){
								childObj.x = _x2 + x1;
							}if(mug == genericMugRight){
								childObj.x = _x3 + x1;
							}
							childObj.y = y1;
